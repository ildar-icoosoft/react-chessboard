{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:update-snapshot": "tsdx test --updateSnapshot",
    "test:generate-output": "tsdx test --passWithNoTests --json --outputFile=.jest-test-results.json || exit 0",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "prebuild-storybook": "npm run test:generate-output",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md,scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "react-chess",
  "author": "ildar",
  "module": "dist/react-chess.esm.js",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.153",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.4.0",
    "babel-loader": "^8.1.0",
    "husky": "^4.2.5",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dnd-test-backend": "^11.1.0",
    "react-dnd-test-utils": "^11.1.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rollup-plugin-postcss": "^3.1.1",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "ramda": "^0.27.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-draggable": "^4.4.3",
    "react-transition-group": "^4.4.1"
  }
}
